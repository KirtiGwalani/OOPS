import java.util.*;
public class Table
{
    public static void main(String[] args) 
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter n: ");
        int n=sc.nextInt();
        System.out.println("the table of "+ n+ " is: ");
        for(int i=1;i<11;i++)
        {
           System.out.println(i*n);
        } 
    }
}
    
