import java.util.*;
public class NumberofDigits
{
    public static void main(String[] args) 
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter n: ");
        int n=sc.nextInt();
        int i=n;
        int count=0;
        while(i>0)
        {
            i=i/10;
            count++;
        }
        System.out.println(count);
    }
}
